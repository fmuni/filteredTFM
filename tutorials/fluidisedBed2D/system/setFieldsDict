FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 1
    volScalarFieldValue alpha.particles 0
);

regions
(
    boxToCell
    {
        box (-0.1335 0 0) (0.1335 0.01 0.9);
        fieldValues
        (
            volScalarFieldValue alpha.air 0.25
            volScalarFieldValue alpha.particles 0.65
        );
    }
);


